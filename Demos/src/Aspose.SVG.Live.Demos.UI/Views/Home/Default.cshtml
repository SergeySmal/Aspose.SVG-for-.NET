
@model  LandingPageModel

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>SVG File Format Solution</h1>
				<h2>On Premise APIs and free apps for SVG file format parsing and rendering with no dependency on any other software.</h2>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose_svg-brand.png" alt="Aspose.SVG" width="320" />
			</div>
			<!--/col6 -->
		</div>
		<!--/row -->
	</div>
	<!--/container -->
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["AsposeSVG"]) </h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>

			
			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_conversion-app.png" alt="Aspose.SVG for .NET" />
					</div>
					<a href="/svg/conversion"><span class="spanclass">@Model.Resources["AsposeSVG"] @Model.Resources["AsposeProductFor"]<em>@Model.Resources["Conversionh4"]</em></span><em>Easily convert SVG files to PDF and many other popular file formats without leaving your favorite browser.</em></a>
				</div>

			</div>			
			
			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_merger-app.png" alt="Aspose.SVG for .NET" />
					</div>
					<a href="/svg/merger"><span class="spanclass">@Model.Resources["AsposeSVG"] @Model.Resources["AsposeProductFor"]<em>Merger</em></span><em>Free app offering features to Merge multiple SVG files.</em></a>
				</div>

			</div>	

		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>
